{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "traverse",
      "camelcase": "Traverse",
      "title": "Traverse",
      "scope": "source.traverse",
      "file-types": [
        "traverse"
      ],
      "injection-regex": "^traverse$",
      "class-name": "TreeSitterTraverse"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "description": "Traverse DSL Parser",
    "authors": [
      {
        "name": "Traverse Team"
      }
    ],
    "links": {
      "repository": "https://github.com/velvet-global/traverse/crates/tree-sitter-traverse"
    }
  },
  "bindings": {
    "rust": true,
  }
}
